    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="22.3" sensor="//@house/@sensors.0"/>
    <actions xsi:type="majordomo:BooleanAction" actor="//@rooms.1/@actors.1"/>
    <actions xsi:type="majordomo:BooleanAction" value="true" actor="//@rooms.1/@actors.0"/>
  </rules>
  <rules>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="17.0" sensor="//@rooms.1/@sensors.0"/>
    <conditions xsi:type="majordomo:GreaterOrEqualFloatCondition" value="9.3" sensor="//@house/@sensors.0"/>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="22.3" sensor="//@house/@sensors.0"/>
    <actions xsi:type="majordomo:BooleanAction" value="true" actor="//@rooms.1/@actors.1"/>
    <actions xsi:type="majordomo:BooleanAction" actor="//@rooms.1/@actors.0"/>
  </rules>
  <rules>
    <conditions xsi:type="majordomo:GreaterOrEqualFloatCondition" value="19.0" sensor="//@rooms.1/@sensors.0"/>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="19.0" sensor="//@house/@sensors.1"/>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="9.3" sensor="//@house/@sensors.0"/>
    <conditions xsi:type="majordomo:GreaterOrEqualFloatCondition" value="22.3" sensor="//@house/@sensors.0"/>
    <actions xsi:type="majordomo:BooleanAction" value="true" actor="//@rooms.1/@actors.0"/>
  </rules>
  <rules>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="17.0" sensor="//@rooms.1/@sensors.0"/>
    <conditions xsi:type="majordomo:GreaterOrEqualFloatCondition" value="17.0" sensor="//@house/@sensors.1"/>
    <conditions xsi:type="majordomo:LesserOrEqualFloatCondition" value="9.3" sensor="//@house/@sensors.0"/>
    <conditions xsi:type="majordomo:GreaterOrEqualFloatCondition" value="22.3" sensor="//@house/@sensors.0"/>
    <actions xsi:type="majordomo:BooleanAction" value="true" actor="//@rooms.1/@actors.0"/>

<?xml version="1.0" encoding="ASCII"?>
<majordomo:Majordomo xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:majordomo="http://mdsd.altimos.de/majordomo/1.0" xsi:schemaLocation="http://mdsd.altimos.de/majordomo/1.0 java://de.altimos.mdsd.majordomo.MajordomoPackage">
  <house>
